<!DOCTYPE html>
<html lang="fa" direction="rtl" dir="rtl" style="direction: rtl">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>Tipoul.AdminPanel.WebUI</title>

	<link href="~/css/Google.fonts.css" async rel="stylesheet" />

	<link href="~/css/persian-datepicker.css" rel="stylesheet" />

	<link href="~/css/fullcalendar.bundle.rtl.css" rel="stylesheet" type="text/css" />
	<link href="~/css/plugins.bundle.rtl.css" rel="stylesheet" type="text/css" />
	<link href="~/css/prismjs.bundle.rtl.css" rel="stylesheet" type="text/css" />
	<link href="~/css/wickedpicker.min.css" rel="stylesheet" type="text/css" />
	<link href="~/css/style.bundle.rtl.css" rel="stylesheet" type="text/css" />
	<link href="~/css/Site.css" rel="stylesheet" type="text/css" />

	<link href="~/favicon.ico" rel="shortcut icon" />

	@await RenderSectionAsync("styles", required: false)
</head>
<body id="kt_body" class="header-fixed header-mobile-fixed sidebar-enabled page-loading">

	@await Html.PartialAsync("_MobileHeader")

	<!--begin::Main-->
	<div class="d-flex flex-column flex-root">
		<!--begin::Page-->
		<div class="d-flex flex-row flex-column-fluid page">

			@await Html.PartialAsync("_SidebarRight")

			<div class="d-flex flex-column flex-row-fluid wrapper" id="kt_wrapper">

				@await Html.PartialAsync("_Header")

				@RenderBody()

				@await Html.PartialAsync("_Footer")

			</div>

			@await Html.PartialAsync("_SidebarLeft")

		</div>
		<!--end::Page-->
	</div>
	<!--end::Main-->
	@await Html.PartialAsync("_Notifications")

	@await Html.PartialAsync("_QuickActions")

	@await Html.PartialAsync("_UserPanel")

	@await Html.PartialAsync("_QuickPanel")

	@await Html.PartialAsync("_Chat")

	@await Html.PartialAsync("_Sticky")

	<!--begin::Scrolltop-->
	<div id="kt_scrolltop" class="scrolltop">
		<span class="svg-icon">
			<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
				<g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
					<polygon points="0 0 24 0 24 24 0 24" />
					<rect fill="#000000" opacity="0.3" x="11" y="10" width="2" height="10" rx="1" />
					<path d="M6.70710678,12.7071068 C6.31658249,13.0976311 5.68341751,13.0976311 5.29289322,12.7071068 C4.90236893,12.3165825 4.90236893,11.6834175 5.29289322,11.2928932 L11.2928932,5.29289322 C11.6714722,4.91431428 12.2810586,4.90106866 12.6757246,5.26284586 L18.6757246,10.7628459 C19.0828436,11.1360383 19.1103465,11.7686056 18.7371541,12.1757246 C18.3639617,12.5828436 17.7313944,12.6103465 17.3242754,12.2371541 L12.0300757,7.38413782 L6.70710678,12.7071068 Z" fill="#000000" fill-rule="nonzero" />
				</g>
			</svg>
		</span>
	</div>
	<!--end::Scrolltop-->

	<script src="~/js/script.js"></script>

	<script src="~/js/plugins.bundle.js"></script>
	<script src="~/js/prismjs.bundle.js"></script>
	<script src="~/js/scripts.bundle.js"></script>
	<script src="~/js/fullcalendar.bundle.js"></script>
	<script src="~/js/widgets.js"></script>
	<script src="~/js/persian-date.js"></script>
	<script src="~/js/persian-datepicker.js"></script>
	<script src="~/js/wickedpicker.min.js"></script>
	<script src="~/js/layout.script.js?1.0.0.1"></script>

	@await RenderSectionAsync("scripts", required: false)
</body>
</html>
